{
	"name"              : "milkyway-multimedia/ss-send-this",
	"description"       : "Another mailer for Silverstripe...",
	"type"              : "silverstripe-module",
	"keywords"          : [
		"silverstripe",
		"email"
	],
	"license"           : "MIT",
	"authors"           : [
		{
			"name"  : "Mellisa Hankins",
			"email" : "mell@milkywaymultimedia.com.au"
		}
	],
	"require"           : {
		"php"                                   : ">=5.4.0",
		"silverstripe/framework"                : "~3.1",
		"phpmailer/phpmailer"                   : "~5.2",
		"guzzlehttp/guzzle"                     : "~4",
		"milkyway-multimedia/ss-hashable"       : "dev-master",
		"milkyway-multimedia/ss-mwm"            : "dev-master",
		"milkyway-multimedia/ss-eventful" : "dev-master"
	},
	"repositories"      : [
		{
			"type" : "vcs",
			"url"  : "https://github.com/milkyway-multimedia/ss-hashable"
		}
	],
	"config"            : {
		"process-timeout" : 600
	},
	"minimum-stability" : "dev",
	"extra"             : {
		"installer-name" : "send-this"
	}
